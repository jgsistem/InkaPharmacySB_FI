<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field" package="app.pucharseorderdetall.domain.entity">
  <class name="Pucharseorderdetall" table="purchase_order_detail">
    <id column="purchase_order_detail_id" name="id">
      <generator class="native"/>
    </id>
    <property column="purchase_order_id" name="purchase_order_id" not-null="true"/>
    <property column="product_id" name="product_id" not-null="true"/> 
    <property column="quantity" name="quantity" not-null="true"/> 
    <property column="cost" name="cost" not-null="true"/>     
    <property column="currency" name="currency" not-null="true"/>        
    
    </class>
</hibernate-mapping>



