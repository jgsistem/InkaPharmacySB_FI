<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="app.employee.domain.entity">
  <class name="Employee" table="employee">
    <id column="employee_id" name="id">
      <generator class="native"/>
    </id>
    <property column="name" name="name"/>
    <property column="last_name1" name="last_name1"/>
    <property column="last_name2" name="last_name2"/>
    <property column="address" name="address"/>    
    <property column="telephone" name="telephone"/>
    <property column="role_id" name="role_id"/>
    <property column="store_id" name="store_id"/>
    <property column="username" name="username"/>    
    <property column="password" name="password"/>
    <property column="email" name="email"/>
    <property column="status" name="status"/>
  </class>
</hibernate-mapping>
